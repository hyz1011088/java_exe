/*
 * This is a java program to calculate the area of a rectangle.
 */
public class square{

	int a;
	int b;
	public square(int num1,int num2){
		this.a=num1;
		this.b=num2;
	}
	public int cal(){
		return a*b;
	}

	public static void main(String[] args){
		square s=new square(2, 4);
		System.out.println(s.cal());
	}

}
